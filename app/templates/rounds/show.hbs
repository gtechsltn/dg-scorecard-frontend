<div class="table-responsive">
  <table class="table table-striped table-condensed">
    <thead>
      <tr>
        <th>Hole</th>
        <th>Score</th>
        <th>Par</th>
      </tr>
    </thead>
    <tbody>
      {{#each model.scores as |score|}}
      <tr>
        <td>{{score.hole.number}}</td>
        <td>{{score.strokes}}</td>
        <td>{{score.hole.par}}</td>
      </tr>
      {{/each}}
      <tr class="info">
        <td>Total</td>
        <td>{{model.score}}</td>
        <td>{{model.course.totalPar}}</td>
      </tr>
    </tbody>
  </table>
</div>
